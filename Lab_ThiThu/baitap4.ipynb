   "source": [
    "import numpy as np\n",
    "Q = np.array([[4, -1, 0],\n",
    "              [1, 3, -2],\n",
    "              [0, 1, 2]])\n",
    "\n",
    "eig_vals, eig_vecs = np.linalg.eig(Q)\n",
    "\n",
    "# Chuẩn hóa các vector riêng\n",
    "eig_vecs_norm = eig_vecs / np.linalg.norm(eig_vecs, axis=0)\n",
    "\n",
    "eig_vals, eig_vecs, eig_vecs_norm"
   ]
  }
